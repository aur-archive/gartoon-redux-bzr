<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/archpkg">
    <comment>Archlinux install package (XZ compressed archive)</comment>
    <sub-class-of type="application/x-xz-compressed-tar"/>
    <generic-icon name="application-x-archpkg"/>
    <glob pattern="*.pkg.tar.xz"/>
  </mime-type>
</mime-info>
